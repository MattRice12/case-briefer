<%= form_for user, html: {multipart: true} do |f| %>
  <fieldset>

    <div>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>

    <div>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <%= f.submit class: 'button' %>
  </fieldset>
<% end %>
